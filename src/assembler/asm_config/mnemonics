nop   -> 00000
mv    -> 00001
li    -> 00010
ld    -> 00011
ldind -> 00100
stio  -> 00101
add   -> 00110
sub   -> 00111
neg   -> 01000
xor   -> 01001
nand  -> 01010
and   -> 01011
or    -> 01100
not   -> 01101
j     -> 01110
jnz   -> 01111
jimm  -> 10000
addi  -> 10001
st    -> 10010
jz    -> 10011
jn    -> 10100